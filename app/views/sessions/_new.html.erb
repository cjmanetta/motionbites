  <%= form_for :session, url: session_path do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email, required: true, placeholder: 'email' %>

    <%= f.label :password %>
    <%= f.password_field :password, required: true, placeholder: 'password' %>

    <%= f.submit 'login', class: 'submit' %>
  <% end %>